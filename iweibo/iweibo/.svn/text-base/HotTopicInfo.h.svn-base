#import <Foundation/Foundation.h>


@interface HotTopicInfo : NSObject {

	NSString *topicName;		// 话题名称
	NSString *topicCount;		// 该话题数量
	NSString *topicLastID;		// 话题在加载更多时候传递上一次请求话题最后一个话题的id
	NSString *hotTopicID;		// 话题id
}

@property (nonatomic, retain) NSString *topicName;
@property (nonatomic, retain) NSString *topicCount;
@property (nonatomic, retain) NSString *topicLastID;
@property (nonatomic, retain) NSString *hotTopicID;

@end
